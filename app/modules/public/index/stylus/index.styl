.header
	height 100px
	position fixed
	top 0px
	left 0px
	right 0px
	z-index 999
	transition all 0.5s ease
	box-shadow 0px 2px 4px rgba(0,0,0,0.1)
	background-color rgba(255,255,255,1.0)
	.headerLogoText
		font-size 42px
		font-family 'Cookie Regular'
		line-height 40px
		height 40px
		position absolute
		left 10px
		top 50%
		margin-top -20px
		padding-left 25px 
		transition all 0.5s ease
		color rgba(54,54,54,1.0)
	.headerBtns
		height 45px
		position absolute
		right 30px
		top 50%
		margin-top -22.5px
		.headerBtn
			font-size 14px
			font-family 'Roboto Regular'
			display inline-block
			padding 15px
			cursor pointer
			border-radius 5px
			color rgba(2, 179, 236, 1.0)
			background-color rgba(233,233,233,1.0)
		.getAppBtn
			margin-right 10px
			color rgba(255,255,255,1.0)
			background-color rgba(2, 179, 236, 1.0)		
/* Larger than mobile */
@media (min-width: 375px)
	.header
		.headerLogoText
			left -180px
/* Larger than phablet */
@media (min-width: 550px)
	.header	
		.headerLogoText
			left 10px
.userProfile
	margin auto
	margin-top 150px
	position relative 
	transition all 0.5s ease
	.profileAvatar
		height 180px
